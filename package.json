{
  "name": "my-ride-link",
  "version": "1.0.0",
  "description": "A comprehensive ride-sharing application with fare comparison, real-time tracking, and payment processing",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --detectOpenHandles",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "build": "echo 'No build step required for Node.js application'",
    "postinstall": "echo 'Post-installation complete'",
    "health": "curl -f http://localhost:3000/health || exit 1"
  },
  "keywords": [
    "ride-sharing",
    "uber",
    "lyft",
    "fare-comparison",
    "real-time-tracking",
    "payment-processing",
    "nodejs",
    "express",
    "mysql",
    "redis"
  ],
  "author": "RideLink Team",
  "license": "Abe Scott",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.2",
    "nodemailer": "^7.0.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.13",
    "sequelize": "^6.37.1",
    "socket.io": "^4.8.1",
    "stripe": "^18.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "express-mongo-sanitize": "^2.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ride-link.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ride-link/issues"
  },
  "homepage": "https://github.com/your-org/ride-link#readme"
}
